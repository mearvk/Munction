<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="presystem" class="system.Presystem">
        <meta key="version" value="1.0"></meta>
    </bean>

    <bean id="system" class="system.System">
        <meta key="version" value="1.0"></meta>
        <constructor-arg ref="presystem"/>
    </bean>

    <bean id="munctionmodule" class="munction.MunctionModule">
        <meta key="version" value="1.0"></meta>
        <constructor-arg ref="presystem"/>
        <constructor-arg ref="system"/>
    </bean>

    <bean id="buildmodule" class="munction.BuildModule">
        <meta key="version" value="1.0"></meta>
        <constructor-arg ref="presystem"/>
        <constructor-arg ref="system"/>
    </bean>

    <bean id="webmodule" class="munction.WebModule">
        <meta key="version" value="1.0"></meta>
        <constructor-arg ref="presystem"/>
        <constructor-arg ref="system"/>
    </bean>

</beans>