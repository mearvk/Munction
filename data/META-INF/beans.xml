<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/aop
           http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd">

    <bean id="presystem" class="system.Presystem"></bean>
    <bean id="system" class="system.System"></bean>
    <bean id="munction" class="munction.Munction"></bean>
    <bean id="buildlistener" class="munction.BuildListener"></bean>
    <bean id="weblistener" class="munction.WebListener"></bean>

</beans>